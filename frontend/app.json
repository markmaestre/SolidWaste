{
  "expo": {
    "name": "WasteWise",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/WasteWise.png",
    "scheme": "WasteWise",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/WasteWises.png",
        "backgroundColor": "#c9d9f0"

      },
      "edgeToEdgeEnabled": true,
      "package": "com.kram_maestre.frontend"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/WasteWise.png",
          "imageWidth": 500,
          "resizeMode": "contain",
         "backgroundColor": "#c9d9f0"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a1ef8d28-aa47-4b60-9536-7d5cb172031a"
      }
    },
    "owner": "kram_maestre"
  }
}
